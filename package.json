{
    "name": "@family-care/common",
    "version": "1.0.8",
    "description": "Common utilities for Family Care",
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.cjs"
    },
    "files": [
        "dist/**/*"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "tsc --project tsconfig.cjs.json && tsc --project tsconfig.esm.json",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "family-care",
        "common",
        "utils"
    ],
    "author": "billypalacios",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^20.0.0",
        "@types/yup": "^0.29.14",
        "typescript": "^5.3.0"
    },
    "dependencies": {
        "undici-types": "^6.19.8",
        "yup": "^1.6.1"
    }
}
